{
  "description": "Polymer CRUD management",
  "keywords": [
    "web-components",
    "polymer",
    "polymer 2",
    "five-elements",
    "air-elements",
    "crud request"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/FiveElements/air-cruddy.git"
  },
  "name": "air-cruddy",
  "version": "0.0.1",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "polymer lint --rules polymer-2   --extra-dependencies bower_components/**/*",
    "analyze": "polymer analyze > analysis.json",
    "dev": "polymer serve -p 8080 | npm run watch",
    "test": "polymer test",
    "dev-install": "npm install -g bower polymer-cli browser-sync",
    "watch": "browser-sync start --proxy localhost:8080 --startPath /components/air-cruddy/demo/ --files *.html,*.js,demo/*"
  },
  "author": "FiveElements",
  "license": "ISC",
  "devDependencies": {
    "@bower_components/air-cascade-validator": "FiveElements/air-cascade-validator#3.0-preview",
    "@polymer/iron-component-page": "^3.0.0-pre.18",
    "@polymer/iron-demo-helpers": "^3.0.0-pre.18",
    "@polymer/iron-icons": "^3.0.0-pre.18",
    "@polymer/paper-icon-button": "^3.0.0-pre.18",
    "@polymer/paper-input": "^3.0.0-pre.18",
    "@polymer/paper-toast": "^3.0.0-pre.18",
    "@polymer/paper-toggle-button": "^3.0.0-pre.18",
    "@webcomponents/webcomponentsjs": "^2.0.0",
    "browser-sync": "^2.18.6",
    "gulp": "^3.9.1",
    "proxy-middleware": "^0.15.0",
    "wct-browser-legacy": "^0.0.1-pre.11"
  },
  "resolutions": {
    "inherits": "2.0.3",
    "samsam": "1.1.3",
    "supports-color": "3.1.2",
    "type-detect": "1.0.0",
    "@webcomponents/webcomponentsjs": "2.0.0-beta.2"
  },
  "dependencies": {
    "@polymer/polymer": "^3.0.0",
    "lodash-es": "^4.17.10"
  }
}
